	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"require": {
		"php": ">= 8.1",
		"nette/application": "^3.2.3",
		"nette/assets": "^1.0.0",
		"nette/bootstrap": "^3.2.6",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10",
		"nettrine/annotations": "^0.7.0",
		"nettrine/dbal": "^0.8.2",
		"nettrine/orm": "^0.8.4",
		"contributte/console": "^0.10.1",
		"fakerphp/faker": "^1.24",
		"contributte/api": "^0.4.0",
		"contributte/middlewares": "^0.12.1",
		"psr/http-server-middleware": "^1.0",
